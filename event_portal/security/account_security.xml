<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Portal for event event -->
        <record id="event_registration_rule_portal" model="ir.rule">
            <field name="name">Portal Personal Event Registration</field>
            <field name="model_id" ref="event.model_event_event"/>
            <field name="domain_force">[('message_partner_ids', 'child_of', [user.commercial_partner_id.id])]</field>
            <field name="groups" eval="[(4, ref('base.group_portal'))]"/>
        </record>
    </data>
</odoo>

