<odoo>
    <record id="'event_portal_ticket_report" model='ir.actions.report'>
        <field name="name">Event Portal Ticket Report</field>
        <field name="model">event.event</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">event_portal_ticket.report_event_ticket_attendees</field>
        <field name="report_file">event_portal_ticket.report_event_ticket_attendees</field>
        <field name="binding_type">report</field>
    </record>

    <template id="portal_my_tickets">
        <t t-call="portal.portal_layout">
            <t t-call="event.model_event_registration"/>
        </t>
    </template>



</odoo>

